
% #58 m-zeroids

assign(max_models, -1).

formulas(assumptions).

% commutative semigroup
(x + y) + z = x + (y + z).
x + y = y + x.

% lattice
(x v y) v z = x v (y v z).
x v y = y v x.
(x ^ y) ^ z = x ^ (y ^ z).
x ^ y = y ^ x.
(x v y) ^ x = x.
(x ^ y) v x = x.

%
-x = x.
x + 0 = 0.
x + -x = 0.
x < y <-> 0 = -x + y.
x + (y v z) = (x + y) v (x + z).

end_of_list.

